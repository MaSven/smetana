public static void agdtdisc(Agraph_s g, _dt_s dict, _dtdisc_s disc) {
    if (disc!=null && NEQ(dtdisc(dict, null, 0), disc)) {
	dtdisc(dict, disc, 0);
    }
    /* else unchanged, disc is same as old disc */
}

