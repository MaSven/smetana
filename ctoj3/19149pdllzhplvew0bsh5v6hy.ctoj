public static __struct__<pointf> scale(final __struct__<pointf> p, double c) {
    p.setDouble("x", p.getDouble("x") * c);
    p.setDouble("y", p.getDouble("y") * c);
    return p;
}

!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!
static Ppoint_t scale(Ppoint_t p, double c)
{
    p.x *= c, p.y *= c;
    return p;
}

