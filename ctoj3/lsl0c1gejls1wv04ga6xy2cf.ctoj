static int ctr = 1;
public static int idmap(Object state, int objtype, CString str, int id[], boolean createflag) {
    CString s;
    if (str!=null) {
	Agraph_s g;
	g = (Agraph_s) state;
	if (createflag)
	    s = agstrdup(g, str);
	else
	    s = agstrbind(g, str);
	id[0] = Memory.identityHashCode(s);
    } else {
	id[0] = ctr;
	ctr += 2;
    }
    return ASINT(N(0));
}

