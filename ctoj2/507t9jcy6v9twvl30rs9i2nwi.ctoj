public static StarVoid dtmemory(Dict_t dt, StarVoid addr, size_t size, Dtdisc_t disc) {
	if(addr!=null)
	{	if(size == null)
		{	free(addr);
			return null;
		}
		else	return realloc(addr,size);
	}
	else	return size.isStrictPositive() ? malloc(size) : null;
}

