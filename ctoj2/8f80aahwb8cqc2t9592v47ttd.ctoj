@TranslatedBy("plantuml")
public static Agsym_t getattr(Agraph_t g, int kind, StarChar name) {
    Agsym_t rv = null;
    Dict_t dict;
    dict = agdictof(g, kind);
    if (dict!=null)
	rv = agdictsym(dict, name);	/* viewpath up to root */
    return rv;
}

